{
  "name": "base-frontend-web",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "nx serve app-client",
    "start:dev": "nx serve app-client & nx build shared-features --watch",
    "build": "nx build app-client",
    "build:libs": "nx build shared-features",
    "watch": "nx build app-client --watch --configuration development",
    "test": "nx test app-client",
    "test:all": "nx run-many --target=test --all",
    "lint": "nx run-many --target=lint --all",
    "format": "nx format:write",
    "format:check": "nx format:check",
    "affected:build": "nx affected --target=build",
    "affected:test": "nx affected --target=test",
    "affected:lint": "nx affected --target=lint",
    "graph": "nx graph",
    "generate:component": "nx generate @nx/angular:component",
    "generate:shared-component": "nx generate @nx/angular:component --path=libs/shared-features/src/lib/shared/components/",
    "generate:module-component": "nx generate @nx/angular:component --path=libs/shared-features/src/lib/modules/"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^20.2.4",
    "@angular/common": "^20.2.4",
    "@angular/compiler": "^20.2.4",
    "@angular/core": "^20.2.4",
    "@angular/forms": "^20.2.4",
    "@angular/platform-browser": "^20.2.4",
    "@angular/platform-browser-dynamic": "^20.2.4",
    "@angular/router": "^20.2.4",
    "@eslint/eslintrc": "^3.3.1",
    "@primeuix/themes": "^1.2.3",
    "@vercel/analytics": "^1.5.0",
    "chart.js": "^4.5.0",
    "primeicons": "^7.0.0",
    "primeng": "^20.1.1",
    "rxjs": "^7.8.2",
    "tslib": "^2.8.1",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/core": "^20.2.2",
    "@angular-devkit/schematics": "^20.2.2",
    "@angular/build": "^20.2.2",
    "@angular/cli": "^20.2.2",
    "@angular/compiler-cli": "^20.2.4",
    "@eslint/js": "^9.35.0",
    "@nx/angular": "21.4.1",
    "@nx/devkit": "21.4.1",
    "@nx/eslint": "21.4.1",
    "@nx/eslint-plugin": "21.4.1",
    "@nx/jest": "21.4.1",
    "@nx/js": "21.4.1",
    "@nx/playwright": "21.4.1",
    "@nx/web": "21.4.1",
    "@nx/workspace": "21.4.1",
    "@playwright/test": "^1.55.0",
    "@schematics/angular": "^20.2.2",
    "@swc-node/register": "~1.11.1",
    "@swc/core": "~1.13.5",
    "@swc/helpers": "~0.5.11",
    "@tailwindcss/postcss": "^4.1.13",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.3.1",
    "@typescript-eslint/utils": "^8.42.0",
    "angular-eslint": "^20.2.0",
    "eslint": "^9.35.0",
    "eslint-config-prettier": "^10.0.0",
    "eslint-plugin-playwright": "^2.2.2",
    "jest": "^30.1.3",
    "jest-environment-jsdom": "^30.1.2",
    "jest-environment-node": "^30.1.2",
    "jest-preset-angular": "~15.0.0",
    "jest-util": "^30.0.5",
    "jsonc-eslint-parser": "^2.1.0",
    "ng-packagr": "~20.2.0",
    "nx": "21.4.1",
    "prettier": "^3.6.2",
    "tailwindcss": "^4.1.13",
    "tailwindcss-primeui": "^0.6.1",
    "ts-jest": "^29.4.1",
    "ts-node": "10.9.2",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.42.0"
  }
}
